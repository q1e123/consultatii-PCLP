#include <iostream>
#include <string.h>
using namespace std;
class Auto {
public:
    Auto(char _marca[10],double _consum)
    {
        consum=_consum;
        int n=strlen(_marca);
        marca=new char[n];
        for(int i=0;i<n;i++)
            marca[i]=_marca[i];
            marca[n]=0;


    }
char* getMarca()
{
    return marca;
}
double getConsum()
{
    return consum;
}
void setMarca(char _marca[10])
{
     int n=strlen(_marca);
        marca=new char[n];
        for(int i=0;i<n;i++)
            marca[i]=_marca[i];
            marca[n]=0;
}
void setConsum(double _consum)
{
    consum=_consum;
}
private:
    char * marca;
    double consum;

};
int main()
{
 Auto h("Dacia",5.2);
 cout<<h.getMarca()<<" ";
 cout<<h.getConsum();
 cout<<endl;
h.setMarca("Renault");
h.setConsum(5.4);
cout<<h.getMarca()<<" ";
 cout<<h.getConsum();

}
